{
  "plugins": [
    "react",
    "prettier"
  ],
  "extends": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "root": true,
  "rules": {
    "prettier/prettier": "error",
  }
}
