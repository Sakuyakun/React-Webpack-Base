  "repository": {
    "type": "git",
    "url": "https://github.com/Sakuyakun/React-Webpack-Boilerplate"
  },
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --config webpack.dev.config.js --progress --colors",
    "build": "cross-env NODE_ENV=production webpack-dashboard -- webpack -p --config webpack.prod.config.js --progress --colors",
    "lint": "eslint ./src/**/*.jsx --color",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "prettier --write",
      "git add"
    ]
  },
